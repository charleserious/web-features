{
	"env": {
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 2016
	},
	"globals": {
		"ajax": true
	},
    "extends": "standard",
    "rules": {
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"semi": ["error", "always"],
		"quotes": ["error", "single"]
	}
}